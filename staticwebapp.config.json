{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/",
      "serve": "/index.html"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 404
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/img/*", "/css/*", "/js/*", "*.{css,js,png,jpg,gif,ico,svg}"]
  },
  "mimeTypes": {
    ".html": "text/html",
    ".css": "text/css",
    ".js": "application/javascript",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".gif": "image/gif",
    ".ico": "image/x-icon",
    ".svg": "image/svg+xml"
  },
  "globalHeaders": {
    "Cache-Control": "no-cache",
    "Access-Control-Allow-Origin": "https://pizzaria-backend-eueqgmb0fyb5cdbj.brazilsouth-01.azurewebsites.net",
    "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
    "Access-Control-Allow-Headers": "Content-Type, Authorization"
  },
  "networking": {
    "allowedIpRanges": []
  }
}
